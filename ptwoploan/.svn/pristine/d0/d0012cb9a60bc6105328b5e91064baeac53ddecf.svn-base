<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<base href="<%=basePath%>">

<title>My JSP 'myaccount.jsp' starting page</title>

<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">
<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
<meta http-equiv="description" content="This is my page">
<!--
	<link rel="stylesheet" type="text/css" href="styles.css">
	-->
<style type="text/css">
table {
	width: 800px;
	height: 80px;
}

.tablet {
	border: 1px #ffa341 solid;
	width: 98%;
	margin: 10px auto 5px auto;
	background: #FFF;
}

.tablet thead {
	height: 26px;
	width: 100%;
}

.tablet thead td {
	background: #ffcd5c;
	font-weight: bold;
	color: #a11407;
	height: 26px;
}

.tablet tbody tr {
	height: 24px;
	line-height: 24px;
	background: #f9f9f9;
}

.tablet tbody tr.oddColor {
	background: #fffde2;
}

.tablet tbody tr.overColor {
	background: #ffb98a;
}

.tablet tfoot td {
	height: 26px;
	line-height: 26px;
	background: #ffeec2;
	text-align: right;
	padding-right: 20px;
}

.alignL {
	text-align: left;
	padding-left: 10px;
}

.alignM {
	text-align: center;
}

.alignR {
	text-align: right;
	padding-right: 10px;
}
</style>
</head>

<body>
	<jsp:include page="/client/head.jsp"></jsp:include>
	<div class="workCov">

		<strong class="conT">当前用户信息</strong>
		<div class="conBox">
			<form>
				<table class="tablet" cellpadding="0" cellspacing="0" border="0">
					<tr>
						<td class="alignM" width="20%">账户</td>
						<td class="alignL">admin</td>
					</tr>
					<tr>
						<td class="alignM" width="20%">真实姓名</td>
						<td class="alignL"><input type="text" name="name"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">联系方式</td>
						<td class="alignL"><input type="text" name="tel"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">联系地址</td>
						<td class="alignL"><input type="text" name="address"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">身份证号</td>
						<td class="alignL"><input type="text" name="idcart"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">公司</td>
						<td class="alignL"><input type="text" name="company"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">工资</td>
						<td class="alignL"><input type="text" name="salary"></td>
					</tr>
					<tr>
						<td class="alignM" width="20%">详细信息</td>
						<td class="alignL"><textarea rows="5" cols="50">
                                
                                </textarea></td>
					</tr>
					<tr>
						<td class="alignM" colspan="2"><input type="button"
							value="修改资料" onclick="location.href='user-password.html'" /> <input
							type="button" value="返回" onclick="javascript:history.back();" />
						</td>
					</tr>
				</table>
				<form>
		</div>
	</div>
	</div>
	<jsp:include page="/client/foot.jsp"></jsp:include>
</body>
</html>
